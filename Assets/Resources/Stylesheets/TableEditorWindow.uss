:root {
  flex-direction: column;
}
:root > #TabContainer {
  background-color: #1b1b1b;
}
:root > #TabContainer #unity-content-container.unity-scroll-view__content-container {
  height: 24px;
  flex-direction: row;
}
:root > #TabContainer.no-tables #unity-content-container.unity-scroll-view__content-container {
  display: none;
}
:root > #TabContents {
  flex-grow: 1;
  padding: 0 4px 4px;
  background-color: #3a3a3a;
}
:root > #TabContents.no-tables {
  justify-content: center;
}
:root > #TabContents.no-tables Label#NoTablesLabel {
  padding: 8px 0;
  align-self: center;
  font-size: 24px;
}
:root > #TabContents.no-tables Button#NoTablesButton {
  padding: 24px 0;
  align-self: center;
  min-width: 200px;
}
:root > #TabContents .contents {
  font-size: 40px;
  -unity-text-align: middle-center;
}

Tab {
  justify-content: center;
  align-items: center;
  margin-right: 2px;
  min-width: 64px;
  border-top-left-radius: 2px;
  border-top-right-radius: 2px;
  border-top-width: 2px;
  border-top-color: #1b1b1b;
  border-bottom-width: 1px;
  border-bottom-color: #1b1b1b;
  background-color: #1b1b1b;
}
Tab.active {
  background-color: #3a3a3a;
  border-top-color: #ff5050;
}
Tab Label.tab-label {
  -unity-text-align: middle-center;
}

